{
  "audio": {
    "targetSampleRate": 16000,
    "targetChannels": 1,
    "chunkMs": 250
  },
  "ingressNormalize": {
    "enabled": true,
    "targetSampleRate": 16000,
    "targetChannels": 1,
    "peakDbfs": -1
  },
  "normalization": {
    "nfkc": true,
    "stripPunct": true,
    "stripSpace": true,
    "lowercase": false
  },
  "storage": {
    "driver": "jsonl",
    "path": "./runs/2025-11-19",
    "retentionDays": 30,
    "maxRows": 100000
  },
  "providers": ["deepgram", "elevenlabs", "local_whisper", "whisper_streaming", "openai"],
  "providerHealth": {
    "refreshMs": 5000
  }
}
